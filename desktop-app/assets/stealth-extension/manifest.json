{
  "manifest_version": 3,
  "name": "Stealth",
  "version": "1.0",
  "description": "Browser stealth",
  "permissions": ["cookies"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["stealth.js"],
      "run_at": "document_start",
      "all_frames": true,
      "world": "MAIN"
    }
  ]
}
